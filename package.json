{
  "name": "runworker",
  "version": "1.0.5",
  "description": "simple worker handler for node js",
  "main": "js/mod.js",
  "directories": {
    "lib": "lib",
    "examples.js": "examples.js",
    "examples.mjs": "examples.mjs"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "esm": "^3.2.6"
  },
  "scripts": {
    "test": "node examples.js/usecase.js",
    "test:mjs": "node -r esm examples.mjs/usecase.mjs",
    "examples.js:test_functions": "node examples.js/test_functions.js",
    "examples.js:usecase_minimalistic": "node examples.js/usecase_minimalistic.js",
    "examples.js:test_functions:debug": "DEBUG=* node examples.js/test_functions.js",
    "examples.js:usecase_minimalistic:debug": "DEBUG=* node examples.js/usecase_minimalistic.js",
    "examples.mjs:test_functions": "node -r esm examples.mjs/test_functions.mjs",
    "examples.mjs:usecase_minimalistic": "node -r esm examples.mjs/usecase_minimalistic.mjs",
    "examples.mjs:test_functions:debug": "DEBUG=* node -r esm examples.mjs/test_functions.mjs",
    "examples.mjs:usecase_minimalistic:debug": "DEBUG=* node -r esm examples.mjs/usecase_minimalistic.mjs"
  },
  "keywords": [
    "cluster",
    "worker",
    "node",
    "task",
    "es6",
    "esm",
    "mjs",
    "modules",
    "export"
  ],
  "author": "Nabil Redmann <repo@bananaacid.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/BananaAcid/runworker.git"
  }
}
